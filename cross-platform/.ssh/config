Include ~/.ssh/private
Include ~/.ssh/work

Host *
  IgnoreUnknown AddKeysToAgent,UseKeychain
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
  PreferredAuthentications publickey

