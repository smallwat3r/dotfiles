Include ~/.ssh/private
Include ~/.ssh/work

Host *
  IgnoreUnknown AddKeysToAgent,UseKeychain
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa

Host github.com
  User git
  Hostname github.com
  PreferredAuthentications publickey

Host pihole
  User pi
  Hostname pihole.local
  PreferredAuthentications publickey

Host nas
  User pi
  Hostname nas.local
  PreferredAuthentications publickey
