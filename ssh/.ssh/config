Include ~/.ssh/private

Host *
  IgnoreUnknown AddKeysToAgent,UseKeychain
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa

Host github.com
  User git
  Hostname github.com
  PreferredAuthentications publickey

Host pi
  User pi
  Hostname rasp-matt.local
  Port 22
  PreferredAuthentications publickey
